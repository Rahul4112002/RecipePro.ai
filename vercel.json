{
  "version": 2,
  "builds": [
    {
      "src": "recipepro/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "recipepro/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "recipepro.settings",
    "PYTHONPATH": "$PYTHONPATH:$PWD"
  },
  "functions": {
    "recipepro/wsgi.py": {
      "includeFiles": "**"
    }
  }
}