{
  "version": 2,
  "builds": [
    {
      "src": "recipepro/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "recipepro/wsgi.py"
    }
  ]
}